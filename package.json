{
  "name": "ts-styled-library",
  "version": "0.2.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
